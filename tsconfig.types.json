  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "openapi-format": [
        "types/openapi-format.d.ts"
      ]
    }
  },
  "include": [
    "test/_types/**/*.ts"
  ]
}
